{
  "name": "kirami-docs",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start",
    "build": "docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext .js,.ts,.jsx,.tsx,.md,.mdx --ignore-path .gitignore",
    "lint:style": "stylelint \"./**/*.{css,scss,sass}\" --ignore-path .gitignore",
    "format": "prettier --write \"./**/*.{html,js,ts,jsx,tsx,json,md,mdx}\"",
    "format:diff": "prettier --list-different \"./**/*.{html,js,ts,jsx,tsx,json,md,mdx}\"",
    "postinstall": "husky install"
  },
  "dependencies": {
    "@docusaurus/core": "^2.4.1",
    "@docusaurus/preset-classic": "^2.4.1",
    "@markprompt/docusaurus-theme-search": "^0.9.0",
    "@mdx-js/react": "^1.6.22",
    "clsx": "^1.2.1",
    "prism-react-renderer": "^1.3.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "^2.4.1",
    "@iconify-json/ri": "^1.1.12",
    "@tsconfig/docusaurus": "^1.0.7",
    "@typescript-eslint/eslint-plugin": "^6.3.0",
    "@typescript-eslint/parser": "^6.3.0",
    "@unocss/eslint-config": "^0.55.0",
    "@unocss/postcss": "^0.55.0",
    "@unocss/preset-icons": "^0.55.0",
    "esbuild": "^0.19.2",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.0",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-mdx": "^2.2.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.33.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "postcss-nesting": "^12.0.1",
    "prettier": "^3.0.1",
    "stylelint": "^15.10.2",
    "stylelint-color-format": "^1.1.0",
    "stylelint-config-css-modules": "^4.3.0",
    "stylelint-config-recess-order": "^4.3.0",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-config-standard-scss": "^10.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.7.0",
    "typescript": "^4.9.5",
    "unocss": "^0.55.0"
  },
  "packageManager": "yarn@3.6.1",
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=18.13"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": "eslint --fix",
    "*.{css,scss,sass}": "stylelint --fix",
    "*.{html,js,ts,jsx,tsx,json,md}": "prettier --write --ignore-unknown"
  }
}
