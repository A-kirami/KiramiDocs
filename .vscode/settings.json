{
  "[javascript][typescript][typescriptreact][javascriptreact][markdown][mdx]": {
    "editor.codeActionsOnSave": {
      "source.fixAll.eslint": true,
      "source.fixAll.stylelint": true
    }
  },
  "[css]": {
    "editor.codeActionsOnSave": {
      "source.fixAll.stylelint": true
    }
  },
  "css.lint.unknownAtRules": "ignore",
  "eslint.validate": ["javascript", "javascriptreact", "typescript", "typescriptreact", "html", "markdown", "mdx"]
}
